  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#888888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9a8ac4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#9a8ac4"
      }
    },
    {
      "scope": ["variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#f5f5f7"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#f5a9b8"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#f4b58a"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#8bd5a2"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "scope": ["constant.language", "variable.language"],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#f5a9b8"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#f4b58a"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": ["meta.brace"],
      "settings": {
        "foreground": "#f5f5f7"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ff6b6b"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#f4b58a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#8bd5a2"
      }
    }
  ]
}
